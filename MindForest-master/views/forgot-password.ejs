<body>
	<section class="fxt-template-animation fxt-template-layout28" data-bg-image="img/figure/background.jpeg">
		<!-- Animation Start Here -->
		<div id="particles-js"></div>
		<!-- Animation End Here -->
		<div class="fxt-content">
			<div class="fxt-header">
				<a href="login-28.html" class="fxt-logo"><img src="img/logo.png" alt="Logo"></a>
				<ul class="fxt-switcher-wrap">
					<li><a href="\login" class="switcher-text">Login</a></li>
					<li><a href="\register" class="switcher-text inline-text">Register</a></li>
					<li><a href="\forgotpassword" class="switcher-text active">Forgot Password</a></li>
				</ul>
			</div>
			<div class="fxt-form">
				<div class="fxt-transformY-50 fxt-transition-delay-1">
					<p>For recover your password</p>
				</div>
				<form method="POST">
					<div class="form-group">
						<div class="fxt-transformY-50 fxt-transition-delay-2">
							<input type="email" class="form-control" name="email" placeholder="Email Address" required="required">
						</div>
					</div>
					<div class="form-group">
						<div class="fxt-transformY-50 fxt-transition-delay-3">
							<button type="submit" class="fxt-btn-fill">Send Me Email</button>
						</div>
					</div>
				</form>
			</div>
	</section>
